#define TAM 5
#include <stdio.h>
#include <stdlib.h>
int main(void){
    unsigned char v1[TAM];
    unsigned char v2[TAM];
    unsigned char d[TAM];
    unsigned char menordistancia;

    for(int i = 0; i<TAM ; i++){
        scanf("%hhu", &v1[i]);
    };
     for(int i = 0; i<TAM ; i++){
        scanf("%hhu", &v2[i]);
    };


    for(int j =0; j<TAM ; j++){
        d[j] = abs(v1[j] - v2[j]);
        
    }
        menordistancia = abs(v1[0] - v2[0]);
    for(int i = 1; i<TAM ; i++){
        menordistancia = (menordistancia> d[i]) ? d[i]: menordistancia;
        
    }

    printf("a menor distancia Ã© %hhu", menordistancia);
    return 0;
}
